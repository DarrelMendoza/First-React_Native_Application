{
  "expo": {
    "name": "DoneWithIt",
    "slug": "donewithit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/darrelicon.png",

    "splash": {
      "image": "./assets/splashtry.jpg",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
